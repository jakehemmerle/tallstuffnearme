// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model FAAObject {
    OASNumber   Int @id
    Verified    String
    Country     String
    State       String
    City        String
    Latitude    String
    Longitude   String
    ObstacleType String
    AGL         Int
    AMSL        Int
    LT          String
    H           String
    AccV        String
    MarInd      String
    FAAStudyNumber String
    Action      String
    JDate       String
}

